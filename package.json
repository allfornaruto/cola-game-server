{
  "name": "cola-game-server",
  "description": "A gameServer based on NodeJS Pinus Framework",
  "version": "1.0.7",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/allfornaruto/cola-game-server"
  },
  "bin": {
    "$": "./dist/bin/app.js"
  },
  "main": "./dist/app",
  "scripts": {
    "start": "node ./dist/app.js",
    "build": "node_modules/.bin/tsc && node copy.js",
    "test": "jest --detectOpenHandles --coverage --runInBand --forceExit --reporters=default --reporters=jest-junit"
  },
  "jest-junit": {
    "outputName": "test-results.xml"
  },
  "dependencies": {
    "@types/bluebird": "^3.5.19",
    "@types/node": "8.10.54",
    "bluebird": "^3.5.1",
    "bufferutil": "^4.0.1",
    "pinus": "1.4.14",
    "reflect-metadata": "^0.1.10",
    "source-map-support": "^0.5.0",
    "utf-8-validate": "^5.0.2",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "cpy": "^8.1.2",
    "jest": "^25.2.4",
    "jest-junit": "^10.0.0",
    "ts-jest": "^25.3.0",
    "tslint": "5.20.1",
    "typescript": "^4.3.5"
  }
}
